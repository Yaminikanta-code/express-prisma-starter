    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.10.5",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.3",
    "nodemon": "^3.0.1",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.3.2"
