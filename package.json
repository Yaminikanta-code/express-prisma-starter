{
  "name": "express-prisma-ts",
  "version": "1.0.0",
  "description": "Express + Prisma TypeScript Boilerplate",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch --clear-screen=false src/index.ts",
